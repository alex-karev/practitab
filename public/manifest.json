{
  "manifest_version": 3,
  "name": "Practitab - New Tab",
  "version": "1.0",
  "author": "Alex Karev",
  "description": "Replaces the default new tab with a more practical, functional and customizable alternative.",
  "icons": {
    "48": "logo48.png",
    "96": "logo96.png"
  },
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "karev.as99@gmail.com",
      "strict_min_version": "42.0"
    }
  }
}
